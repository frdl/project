{
	"name": "frdl\/project",
	"type": "project",
	"description": "Kickstarter boilerplate to be used by `composer create-project` command. Demo/Test Application. ",
	"keywords" : ["1.3.6.1.4.1.37553.8.1.8.8.11", "1.3.6.1.4.1.37553.8.1.8.8.11.1"],
	"license": ["MIT"],
	"homepage" : "https://github.com/frdl/project", 
	"repositories": [
        {
            "type": "composer",
            "url": "https:\/\/packages.frdl.de"
        }  
	],
	"require": {
		"php": ">=7.2",
		"frdl\/frdlweb-composer-installers": "*"
	},
        "require-dev": {
            "frdl/remote-psr4" : "*",
	    "frdl/console" : "*",
	    "frdl/cronjobs" : "*",
	    "roave/security-advisories": "dev-master",
	    "frdl/frdlweb-software-support" : "*",
	    "symfony/finder" : "^5.1",
	    "symfony/filesystem" : "^5.1"		
        },	    
    "autoload": {
        "psr-4": {
            "compiled\\": "compiled\/"
        }
    },
    "config": {
        "preferred-install": "auto",
        "cache-files-ttl": 3600,
        "prepend-autoloader": true,
        "platform": {
            "php": "7.3.18"
        }
    },
    "conflict": {},
    "extra": {
        "installer-types": [
            "frdl-theme",
            "frdl-module",
            "frdl-httpdocs",
            "frdl-public",
            "frdl-extension",
            "vendor",
            "npm-package",
            "node-module",
            "project"
        ],
        "installer-paths": {
            "modules\/{$vendor}.{$name}\/": [
                "type:frdl-module"
            ],
            "frdl-extensions\/{$vendor}.{$name}\/": [
                "type:frdl-extension"
            ],
            "themes\/{$vendor}\/{$name}\/": [
                "type:frdl-theme"
            ],
            "..\/..\/..\/httpdocs\/{$vendor}\/{$name}\/": [
                "type:frdl-httpdocs"
            ],
            "{$webroot}\/": [
                "type:frdl-public"
            ],
            "vendor\/frdlweb\/{$vendor}\/{$name}\/": [
                "vendor:frdl",
                "vendor:frdlweb",
                "vendor:webfan",
                "vendor:webfan3"
            ],
            "web\/{$vendor}\/{$name}\/": [
                "type:frdl-httpdocs"
            ],
            "{$workspace}\/node_modules\/{$vendor}\/{$name}\/": [
                "type:node-module"
            ],
            "assets\/node_modules\/{$vendor}\/{$name}\/": [
                "type:npm-package"
            ],
            "..\/{$vendor}~{$name}~project\/": [
                "type:project"
            ]
        },
        "frdl": {
            "level": "level-2",
            "webfantize": {
	       "flavor" : "frdl-project",
	       "oid" : "1.3.6.1.4.1.37553.8.1.8.8.11.1"
	    }
        },
        "config-plugin-output-dir": "config\/",
        "dependency-scripts": {
            "run": true,
            "trust": [
                "symfony",
                "symfony\/symfony",
                "symfony\/console",
                "frdl\/console",
                "frdl\/*",
                "webfan3\/*"
            ],
            "exclude": [],
            "types": []
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "pre-autoload-dump": [
            "Webfan\\Support\\ComposerInstallersExtender\\PackageUpdater::processCommandEvent"
        ],
        "pre-update-cmd": [
            "Webfan\\Support\\ComposerInstallersExtender\\PackageUpdater::processBeforeInstallOrUpdate"
        ],
        "pre-install-cmd": [
            "Webfan\\Support\\ComposerInstallersExtender\\PackageUpdater::processBeforeInstallOrUpdate"
        ],
        "post-root-package-install": [
            "Webfan\\Support\\ComposerInstallersExtender\\PackageUpdater::processCommandEvent"
        ],
        "post-create-project-cmd": [
            "Webfan\\Support\\ComposerInstallersExtender\\PackageUpdater::processCommandEvent"
        ],
        "post-autoload-dump": [
            "Webfan\\Support\\ComposerInstallersExtender\\PackageUpdater::processCommandEvent",
            "Webfan\\Support\\ComposerInstallersExtender\\PackageUpdater::processAfterInstallOrUpdate",
            "Frdlweb\\Command\\CompileEvents::onInvoke",
            "Frdlweb\\Command\\BuildConfigWizard::onInvoke",
            "@composer frdl:compile-events",
            "@composer frdl:build-configs"
        ],
        "post-install-cmd": [
            "Webfan\\Support\\ComposerInstallersExtender\\PackageUpdater::processCommandEvent",
            "Webfan\\Support\\ComposerInstallersExtender\\PackageUpdater::processAfterInstallOrUpdate"
        ],
        "post-update-cmd": [
            "Webfan\\Support\\ComposerInstallersExtender\\PackageUpdater::processCommandEvent",
            "Webfan\\Support\\ComposerInstallersExtender\\PackageUpdater::processAfterInstallOrUpdate"
        ],
        "pre-package-uninstall": [
            "Webfan\\Support\\ComposerInstallersExtender\\PackageUpdater::processPackageEvent"
        ],
        "post-package-update": [
            "Webfan\\Support\\ComposerInstallersExtender\\PackageUpdater::processPackageEvent"
        ],
        "pre-package-update": [
            "Webfan\\Support\\ComposerInstallersExtender\\PackageUpdater::processPackageEvent"
        ],
        "post-package-install": [
            "Webfan\\Support\\ComposerInstallersExtender\\PackageUpdater::processPackageEvent"
        ],
        "pre-package-install": [
            "Webfan\\Support\\ComposerInstallersExtender\\PackageUpdater::processPackageEvent"
        ],
        "fix": [],
        "lint": [],
        "test": [],
        "post-package-uninstall": [
            "Webfan\\Support\\ComposerInstallersExtender\\PackageUpdater::processPackageEvent"
        ],
        "frdlweb": "@php vendor\/bin\/frdlweb",
        "frdl": "vendor\/bin\/frdl",
        "frdl:compile-events": [
            "Frdlweb\\Command\\CompileEvents::onInvoke"
        ],
        "frdl:build-configs": [
            "Frdlweb\\Command\\BuildConfigWizard::onInvoke"
        ]
    }
}
