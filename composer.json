{
 "name": "frdl/project",
 "type": "project",
 "description": "Kickstarter boilerplate to be used by `composer create-project` command. Demo/Test Application. ",
 "keywords" : ["1.3.6.1.4.1.37553.8.1.8.8.11", "1.3.6.1.4.1.37553.8.1.8.8.11.1"],
 "license": ["MIT"],
 "homepage" : "https://github.com/frdl/project", 
 "repositories": [
        {
            "type": "composer",
            "url": "https:\/\/packages.frdl.de"
        }  

 ], 
"require": {
        "php": ">=7.2 | >=8.0",
        "frdl/webfan": "*",
        "roave/security-advisories": "dev-master"
    },
     
    "autoload": {
        "psr-4": {
            "compiled\\": "compiled\/"
        }
    },
    "config": {
        "preferred-install": "auto",
        "cache-files-ttl": 3600,
        "prepend-autoloader": true,
        "platform": {
            "php": "7.3.20"
        }
    },
    "conflict": {},
    "extra": {
        "webfantize": {
            "flavor": "frdl-project",
            "oid": "1.3.6.1.4.1.37553.8.1.8.8.11.1"
        },
        "installer-types": [
            "frdl-theme",
            "frdl-module",
            "frdl-httpdocs",
            "frdl-public",
            "vendor",
            "frdl-extension",
            "npm-package",
            "node-module",
            "project"
        ],
        "installer-paths": {
            "modules\/{$vendor}.{$name}\/": [
                "type:frdl-module"
            ],
            "frdl-extensions\/{$vendor}.{$name}\/": [
                "type:frdl-extension"
            ],
            "themes\/{$vendor}\/{$name}\/": [
                "type:frdl-theme"
            ],
            "{$webroot}\/": [
                "type:frdl-public"
            ],
            "vendor\/frdlweb\/{$vendor}\/{$name}\/": [
                "vendor:frdl",
                "vendor:frdlweb",
                "vendor:webfan",
                "vendor:webfan3"
            ],
            "{$workspace}\/node_modules\/{$vendor}\/{$name}\/": [
                "type:node-module"
            ],
            "assets\/node_modules\/{$vendor}\/{$name}\/": [
                "type:npm-package"
            ],
            "..\/{$vendor}~{$name}~project\/": [
                "type:project"
            ],
            "web\/{$vendor}\/{$name}\/": [
                "type:frdl-httpdocs"
            ]
        },
        "dependency-scripts": {
            "run": true,
            "trust": [
                "symfony",
                "symfony\/symfony",
                "symfony\/console",
                "frdl\/console",
                "frdl\/*",
                "webfan3\/*",
                "wehowski\/*"
            ],
            "exclude": [],
            "types": [
                "frdl-extension",
                "frdl-theme",
                "frdl-module"
            ]
        },
        "config-plugin-output-dir": "config",
        "frdl": {
            "webfantize": [],
            "level": "level-2"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {

    }
}
