{
 "name": "frdl/project",
 "type": "project",
 "description": "Kickstarter boilerplate to be used by `composer create-project` command. Demo/Test Application. ",
 "keywords" : ["1.3.6.1.4.1.37553.8.1.8.8.11", "1.3.6.1.4.1.37553.8.1.8.8.11.1"],
 "license": ["MIT"],
 "homepage" : "https://github.com/frdl/project", 
 "repositories": [
         {
            "type": "path",
            "url": "~/.webfan/red/packages"
        },
         {
            "type": "path",
            "url": "~/.webfan/green/packages"
        },
         {
            "type": "path",
            "url": "~/.webfan/blue/packages"
        },
         {
            "type": "path",
            "url": "~/.webfan/red/modules/extension"
        },
         {
            "type": "path",
            "url": "~/.webfan/green/modules/extension"
        },
         {
            "type": "path",
            "url": "~/.webfan/blue/modules/extension"
        },
        {
            "type": "composer",
            "url": "https:\/\/packages.frdl.de"
        }  

  
  
     "frdl/array-helper": {
        "url": "https://github.com/frdl/array-helper/archive/refs/heads/main.zip",
        "path": "~/.webfan/red/packages/frdl/array-helper",
        "ignore": [".*"]
      },
      "vendor-patch/psecio.secure_dotenv": {
        "url": "https://github.com/vendor-patch/secure_dotenv/archive/refs/heads/master.zip",
        "path": "~/.webfan/red/modules/extension/vendor-patch/secure_dotenv",
        "ignore": [".*"]
      },
      "vendor-patch/wikimedia.composer-merge-plugin": {
        "url": "https://github.com/vendor-patch/composer-merge-plugin/archive/refs/heads/master.zip",
        "path": "~/.webfan/red/modules/extension/vendor-patch/secure_dotenv",
        "ignore": [".*"]
      }
    }
  
  
  
  
  
  
 ], 
"require": {
        "php": ">=7.2|>=8.0",
        "frdl/webfan": "*",
        "roave/security-advisories": "dev-master"
 },  
 "suggest": {
        "frdl/project" : "Frdlweb Project Template",        
        "foxy/foxy": "Manage npm depencencies/assets",
        "frdlweb/app-implementation" : "Implements \\frdlweb\\AppInterface",
        "frdlweb/runtime-implementation" : "Implements runtime",
        "frdlweb/workspace-implementation" : "Implements workspace"
    },      
  "autoload": {
        "psr-4": {
            "compiled\\": "compiled\/"
        }
  },
  "config": {
        "bin-dir": "~/.webfan/runtime/.bin",
        "preferred-install": "auto",
        "cache-files-ttl": 3600,
        "prepend-autoloader": true
 },
 "conflict": {},
 "extra": {
    "compile-mode": "all",     
    "foxy": true,
    "downloads": {
      "*": {
        "path": "./vendor/{$id}",
        "ignore": ["test", "tests", "doc", "docs", ".*"]
      }     
    },
        "webfantize": {
            "flavor": "frdl-project",
            "oid": "1.3.6.1.4.1.37553.8.1.8.8.11.1"
        },
        "installer-types": [
            "frdl-extension",
            "frdl-theme",
            "frdl-module",
            "frdl-httpdocs",
            "frdl-public",         
            "webfan-package",
            "webfan-extension",
            "webfan-theme",
            "webfan-module",
            "webfan-httpdocs",
            "webfan-public",         
            "vendor",
            "npm-package",
            "node-module",
            "project"
        ],
        "installer-paths": {
            "modules\/{$vendor}.{$name}\/": [
                "type:frdl-module"
            ],
            "frdl-extensions\/{$vendor}.{$name}\/": [
                "type:frdl-extension"
            ],
            "themes\/{$vendor}\/{$name}\/": [
                "type:frdl-theme"
            ],
            "{$webroot}\/": [
                "type:frdl-public"
            ],
            "vendor\/frdlweb\/{$vendor}\/{$name}\/": [
                "vendor:frdl",
                "vendor:frdlweb",
                "vendor:webfan",
                "vendor:webfan3"
            ],
            "{$workspace}\/node_modules\/{$vendor}\/{$name}\/": [
                "type:node-module"
            ],
            "assets\/node_modules\/{$vendor}\/{$name}\/": [
                "type:npm-package"
            ],
            "..\/{$vendor}~{$name}~project\/": [
                "type:project"
            ],
            "web\/{$vendor}\/{$name}\/": [
                "type:frdl-httpdocs"
            ]
        },
        "dependency-scripts": {
            "run": true,
            "trust": [
                "symfony",
                "symfony\/symfony",
                "symfony\/console",
                "frdl\/console",
                "frdl\/*",
                "webfan3\/*",
                "wehowski\/*"
            ],
            "exclude": [],
            "types": [
                "frdl-extension",
                "frdl-theme",
                "frdl-module",
                "webfan-extension",
                "webfan-theme",
                "webfan-module",
                "webfan-package"
            ]
        },
        "config-plugin-output-dir": "config",
        "frdl": {
            "webfantize": [],
            "level": "level-*"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "command": [
            "@composer webfantize --firstrunwizard ~/.webfan"
        ]
    }
}
